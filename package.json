{
  "name": "steam-robot",
  "version": "1.0.0",
  "description": "Creating steam bots based on middlewares",
  "keywords": [
    "steam",
    "bot",
    "middlewares"
  ],
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/steam-robot.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "yarn run docs:typedoc && yarn run docs:concat",
    "docs:typedoc": "typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs --name API src/index.ts",
    "docs:concat": "concat-md --hide-anchor-links docs > README.md",
    "test": "yarn run test:typescript && yarn run test:eslint && yarn run test:ncu",
    "test:typescript": "tsc --noEmit",
    "test:eslint": "eslint .",
    "test:ncu": "ncu --errorLevel 2"
  },
  "dependencies": {
    "@types/request": "^2.48.8",
    "@types/steam-totp": "^2.1.0",
    "@types/steam-user": "^4.26.0",
    "@types/steamcommunity": "^3.43.2",
    "request": "^2.88.2",
    "steam-market": "^1.0.0",
    "steam-totp": "^2.1.2",
    "steam-tradeoffer-manager": "^2.10.5",
    "steam-user": "^4.26.0",
    "steamcommunity": "^3.44.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concat-md": "^0.5.0",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "npm-check-updates": "^16.4.3",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.9.3"
  }
}
